{
  "name": "fastify-node-config",
  "version": "1.0.0",
  "description": "Plugin to help integrate node config with Fastify",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "repository": "git@github.com:nholik/fastify-node-config.git",
  "author": "Nick Holik <nick@nlh3.com>",
  "license": "MIT",
  "packageManager": "yarn@3.6.0",
